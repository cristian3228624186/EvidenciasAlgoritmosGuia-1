Proceso Ejemplo05
	//Solicitar 2 números y mostrar la suma de ambos
	//DECLARACIÓN/DEFINICIÓN VARIABLES
	Definir num1, num2, resultado Como Real;
	leer num1;
	Escribir "Digite el valor del número 2";
	leer num2;
	//PROCESO-OPERACIONES-FORMULAS
	resultado <- num1 + num2;
	
	//SALIDA DATOS
	Escribir "El resultado de suma ", num1, " + ",num2," es: ",resultado;
	
FinProceso