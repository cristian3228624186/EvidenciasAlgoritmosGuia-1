Proceso Ejemplo04
	//Se requiere un algoritmo que permita solicitar un número de usuario y calcular la siguiente formula: x <- 2A + A*5
	//DECLARACIÓN/DEFINICIÓN VARIABLES
	Definir X , A Como Real;
	//ENTRADA DATOS
	Escribir "Digite el valor del número";
	Leer A;
	
	//PROCESO-OPERACIONES-FORMULAS
	X <- 2*A+A*5;
	
	//SALIDAS DATOS
	Escribir "El resultado de la operación", X;
	
FinProceso